<div class="list">
<h1 class="title">My Photos</h1>
    <a ng-repeat="list in datalits" class="item item-thumbnail-left" href="#">
      <img src="{{list.pic}}">
      <h2>{{list.fname}}</h2>
      <p>{{list.lname}}</p>
    </a>
	

</div>
<div class="tabs tabs-icon-only">
  <a ui-sref="login" class="tab-item">
    <i class="icon ion-home"></i>
  </a>
  <a ui-sref="history" class="tab-item">
    <i class="icon ion-person"></i>
  </a>
  <a ui-sref="img" class="tab-item">
    <i class="icon ion-images"></i>
  </a>
  <a ui-sref="li" class="tab-item">
    <i class="icon ion-star"></i>
  </a>
  <a ui-sref="set" class="tab-item">
    <i class="icon ion-gear-a"></i>
  </a>
  
</div>
